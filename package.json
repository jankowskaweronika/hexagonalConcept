{
  "name": "hexagonal-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "glob -c \"TS_NODE_PROJECT=tsconfig.test.json node -r ts-node/register --test \" \"src/**/*.test.ts\" \"application/**/*.test.ts\" \"tests/**/*.test.ts\"",
    "test:watch": "glob -c \"TS_NODE_PROJECT=tsconfig.test.json node -r ts-node/register --watch --test\" \"src/**/*.test.ts\" \"application/**/*.test.ts\" \"tests/**/*.test.ts\"",
    "test:watch:only": "glob -c \"TS_NODE_PROJECT=tsconfig.test.json node -r ts-node/register --watch --test --test-only\" \"src/**/*.test.ts\" \"application/**/*.test.ts\" \"tests/**/*.test.ts\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.19",
    "axios": "^1.6.8",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "glob": "^10.3.12",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
